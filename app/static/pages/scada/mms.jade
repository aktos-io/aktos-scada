  p Ä°yidir
  a(href='#mms') Sayfa-0
  a(href='#mms-2') Sayfa-2
  
#mms-2(data-role='page', data-theme='b')
  p Senden
  a(href='#mms') Sayfa-0
  a(href='#mms-1') Sayfa-1
  