        {{>justgage {pin_name: 'vpin-2', val: 45, wid: 401} }}
        {{>numeric-keyboard {wid: 402} }}
        
        {{>b-stacked-progress 
          {pin_names: [ 
              ['vpin-2', 'red'], 
