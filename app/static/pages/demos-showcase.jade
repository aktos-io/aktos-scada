        {{! graph example }}
        {{>line-graph {pin_name: 'vpin-2', wid: 5} }}
        {{>slider {pin_name: 'vpin-2', wid: 6} }}
        
        {{>thermometer {pin_name: 'vpin-2', wid: 8} }}
        
        {{>b-vertical-progress-bar {pin_name: 'vpin-3', wid: 9} }}
        {{>slider {pin_name: 'vpin-3', wid: 10} }}
